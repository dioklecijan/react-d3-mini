{
  "name": "react-d3-mini",
  "version": "1.0.0",
  "description": "Rating control",
  "repository": "git+https://github.com/dioklecijan/react-d3-rating.git",
  "author": "Mario Gudelj",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build:css": "postcss src/styles.css -o lib/styles.css --use autoprefixer",
    "build:js": "npm run clean && cross-env NODE_ENV=es babel src --out-dir lib",
    "build": "npm run build:js && npm run build:css",
    "lint": "eslint 'src/**/*.js'",
    "prettier": "prettier --write 'src/**/*.js'",
    "typecheck": "flow check",
    "check": "yarn prettier && yarn lint && yarn flow",
    "test": "jest --no-watchman --runInBand",
    "gh-pages": "./node_modules/.bin/gh-pages -d styleguide",
    "styleguide": "styleguidist server",
    "styleguide:build": "cross-env NODE_ENV=production styleguidist build",
    "publish-docs": "npm run styleguide-build && npm run gh-pages",
    "publish-patch": "npm test && npm run build && npm version patch && npm publish",
    "publish-minor": "npm test && npm run build && npm version minor && npm publish",
    "publish-major": "npm test && npm run build && npm version major && npm publish"
  },
  "babel": {
    "presets": [
      "./.babelrc.js"
    ]
  },
  "files": [
    "dist",
    "styles.css"
  ],
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "d3",
    "rating"
  ],
  "bugs": {
    "url": "https://github.com/dioklecijan/react-d3-rating/issues"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "/tests/.*_spec\\.js",
    "roots": [
      "./tests",
      "./src"
    ],
    "setupTestFrameworkScriptFile": "./tests/init.js",
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coveragePathIgnorePatterns": [
      "./node_modules/",
      "./tests/"
    ],
    "setupFiles": [
      "raf/polyfill"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^7.2.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-jest": "^22.0.4",
    "babel-loader": "7.1.2",
    "babel-plugin-flow-react-proptypes": "^13.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "codecov": "^2.2.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.40.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-relay": "^0.0.21",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "flow-bin": "^0.63.1",
    "gh-pages": "^1.0.0",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.0.4",
    "postcss": "^6.0.14",
    "postcss-cli": "^4.1.1",
    "postcss-loader": "^2.0.9",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-styleguidist": "^6.5.3",
    "rimraf": "^2.4.3",
    "style-loader": "^0.19.1",
    "watch": "^1.0.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "d3-selection": "^1.3.0",
    "d3-transition": "^1.1.1",
    "loose-envify": "^1.3.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0-alpha",
    "react-dom": "^15.3.0 || ^16.0.0-alpha"
  },
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  }
}
